{
  "name": "Questlog",
  "repository": "https://github.com/mockdeep/questlog",
  "dependencies" : {
    "browserify": "9.0.8",
    "browserify-incremental": "2.0.0",
    "es5-shim": "4.1.1",
    "react": "0.13.1",
    "react-dnd": "0.9.8",
    "react-router": "0.13.2",
    "reactify": "1.1.0",
    "reqwest": "1.1.5"
  },
  "devDependencies": {
    "jest-cli": "0.4.0",
    "react-tools": "0.13.1"
  },
  "engines": {
    "node": "0.10.38"
  },
  "scripts": {
    "test": "node ./node_modules/jest-cli/bin/jest.js"
  },
  "jest": {
    "rootDir": "./spec/javascripts",
    "scriptPreprocessor": "<rootDir>/preprocessor.js",
    "setupTestFrameworkScriptFile": "<rootDir>/test_helper.js",
    "moduleFileExtensions": [ "js", "jsx"],
    "unmockedModulePathPatterns": [
      "react"
      ],
    "testFileExtensions": ["js", "jsx"],
    "testPathIgnorePatterns": [ "preprocessor.js" ]
  }
}
