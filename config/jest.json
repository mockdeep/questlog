{
  "clearMocks": true,
  "collectCoverageFrom": ["app/javascript/**/*.js"],
  "coverageReporters": ["json", "lcov"],
  "coverageThreshold": {
    "global": {
      "branches": 50.88,
      "functions": 58.84,
      "lines": 63.19,
      "statements": 57.12
    }
  },
  "moduleFileExtensions": ["js", "jsx"],
  "modulePaths": [
    "<rootDir>/app/javascript",
    "<rootDir>/vendor/assets/javascripts/",
    "<rootDir>/spec/javascript/"
  ],
  "rootDir": "../",
  "setupTestFrameworkScriptFile": "<rootDir>/spec/javascript/test_helper.js",
  "testRegex": "/spec/javascript/.*_spec.js(x)?$",
  "testURL": "http://test.host"
}
